{
  "name": "aq_backend",
  "version": "1.0.0",
  "description": "This tool will check AirQuality around you and prompt about it",
  "main": "start.ts",
  "scripts": {
    "cc": "npx prettier --write ./src/",
    "ct": "npx prettier --write ./test/",
    "start": "ts-node ./src/start.ts",
    "build": "tsc -p .",
    "dev": "nodemon -r tsconfig-paths/register src/",
    "test": "mocha --check-leaks -r tsconfig-paths/register ts-node/register \"test/**/*.spec.ts\""
  },
  "keywords": [
    "Air",
    "Quality",
    "Control"
  ],
  "author": "silassentinel",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "chai": "^4.3.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.20",
    "prettier": "2.4.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
